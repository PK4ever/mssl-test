<div class="container-fluid" id="section1" >
    <div class="row">
        <div class="col-sm-4"></div>
        <div class="col-sm-4">
            <img class="img-responsive center-block" id="logo" src="images/UEFAChampionsLeague1.png">
        </div>
        <div class="col-sm-4">

        </div>
    </div>
<br>
<br>
<br>
    <h1 class="text-center" style="font-family: georgia; font-weight: 800; color: #d9edf7; text-shadow: 0px 10px 4px black">UPCOMING GAMES</h1>
<div class="container text-center" style="background: rgba(0,0,0,.5) !important;">
        #foreach($game in $upcomingGames)

            <div class="container" style="margin-left: -30px;margin-bottom: -50px">
            <di class="row text-center" style="font-family: georgia; font-weight: 800; font-size: large;">
                <div>
                    DATE: $game.getDate()
                </div>
                <div>
                    TIME: $game.getTime()
                </div>
            </di>
            </div>
            <div class="container upcomingGames" style="margin-left: -100px">
            <di class="row" style="font-family: georgia; font-weight: 800;">
                <div class="col-sm-4"></div>
                <div class="col-sm-4 text-center upcomingGames2 setmargin1" style="font-size: 40px; width: 200px; text-shadow: 0px 10px 4px black"">
                    $game.getTeam1().getName()
                </div>
                <div class="col-sm-4 text-center vs" class="row" style="font-size: 60px; width:10%;color: #d9edf7; text-shadow: 0px 10px 4px black">
                    VS
                </div>
                <div class="col-sm-4 text-center upcomingGames2 setmargin2" style="font-size: 40px; width:200px;text-shadow: 0px 10px 4px black">
                    $game.getTeam2().getName()
                </div>
            </di>
            </div>
        #end
</div>
</div>

</div>
<div class="container-fluid" id="section2" >
##    <h1>Fixtures/Results</h1>
    <div class="container">
    <h1 style="font-family: georgia; font-weight: 900;text-shadow: 0px 5px 0px black; color: #d9edf7">GAME SCHEDULE AND RESULTS</h1>
        <table id="schedule" class="table">
            <thead>
            <tr style="font-family: georgia; font-size: x-large; text-shadow: 0px 5px 0px black; color: #d9edf7">
                <th class="col-md-1">DATE</th>
                <th class="col-md-1" >TIME</th>
                <th class="col-md-1" >HOME</th>
                <th class="col-md-1" >SCORE</th>
                <th class="col-md-1">AWAY</th>
                <th class="col-md-1">FIELD</th>

            </tr>
                #foreach($game in $games)
                <tr style="font-family: georgia; color: white; font-size: x-large">
                    <td>
                        $game.getDate()
                    </td>
                    <td>
                        $game.getTime()
                    </td>
                    <td>
                        $game.getTeam1().getName()
                    </td>
                    <td>
                        $game.getScore()
                    </td>
                    <td>
                        $game.getTeam2().getName()
                    </td>
                    <td>
                        $game.getField()
                    </td>
                </tr>
                #end
            </thead>
        </table>

        </div>
</div>
<div class="container-fluid" id="section3" >
##    <h1>Standings</h1>
    <div class="container">
        <h1 style="font-family: georgia; font-weight: 900;text-shadow: 0px 10px 4px black; color: #d9edf7">STANDINGS</h1>
        <table id="schedule" class="table">
            <thead>
            <tr style="font-family: georgia; font-size: x-large; text-shadow: 0px 10px 4px black; color: #d9edf7">
                <th class="col-md-1">POS</th>
                <th class="col-md-1">TEAM</th>
                <th class="col-md-1">P</th>
                <th class="col-md-1" >W</th>
                <th class="col-md-1" >D</th>
                <th class="col-md-1" >L</th>
                <th class="col-md-1">GF</th>
                <th class="col-md-1">GA</th>
                <th class="col-md-1">GD</th>
                <th class="col-md-1">PTS</th>

            </tr>
                #foreach($team in $Teams)
                <tr style="font-family: georgia; color: white; font-size: x-large">
                    <td>
                        $team.getPosition()
                    </td>
                    <td>
                        $team.getName()
                    </td>
                    <td>
                        $team.getPlayed()
                    </td>
                    <td>
                        $team.getWins()
                    </td>
                    <td>
                        $team.getDraws()
                    </td>
                    <td>
                        $team.getLoses()
                    </td>
                    <td>
                        $team.getGoalsFor()
                    </td>
                    <td>
                        $team.getGoalsAgainst()
                    </td>
                    <td>
                        $team.getGoalDifferential()
                    </td>
                    <td>
                        $team.getPoints()
                    </td>
                </tr>
                #end
            </thead>
        </table>

    </div>
</div>

<div class="container-fluid" id="section4" >
##    <h1>Rosters</h1>
    <div class="container-fluid">
        <h1 style="font-family: georgia; font-weight: 900;text-shadow: 0px 5px 0px black; color: #d9edf7">Teams and Rosters</h1>
        <div class="row">
        #foreach($team in $Teams)
            <div class="col-6 col-sm-2">
            <table class="table table-bordered">
                <tr>
                    <th class="text-center" style="color:white;text-shadow: 10px 10px 6px black;box-shadow: 10px 10px 6px black">
                        <img src="images/logo1.png"/>
                        $team.getName()
                    </th>
                </tr>
                #foreach($player in $team.getPlayers())
                    <tr style="box-shadow: 10px 10px 6px black">
                        <td style="color:white;text-shadow: 10px 10px 6px black">
                            $player.getFullName()
                        </td>
                    </tr>
                #end
            </table>
            </div>
        #end
        </div>
    </div>
</div>
